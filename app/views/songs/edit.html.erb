<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title, "Title:"%>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name%>
  <%= f.text_field :artist_name %><br>
  <%= f.label :genre_name, "Genre Name:" %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <%= f.label :note_contents, "Notes:" %>
      <input name="song[note_contents][]">
      <input name="song[note_contents][]">
      <input name="song[note_contents][]">
      <input type="submit" value="Submit">
      <% end %>
  <%# <%= f.label :note_contents, "Notes:" %>
  <%# <%= f.text_field :note_contents %> %> %>
  <br />

  <%= f.submit %>
<% end %>
